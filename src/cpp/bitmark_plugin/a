g++ -shared -fpic -o peer_data.so -g peer_data.cpp -lboost_system -lboost_thread -ltorrent-rasterbar bitmark_plugin/bitmark_peer_data.cpp bitmark_plugin/tweetnacl.o -Wall -fpermissive -l json -lcurl

g++ -shared -fpic -o session.so -g session.cpp -lboost_system -lboost_thread -ltorrent-rasterbar bitmark_plugin/bitmark_plugin.cpp bitmark_plugin/bitmark_peer_data.cpp bitmark_plugin/tweetnacl.o -Wall -fpermissive -l json -lcurl


g++ -lboost_system -lboost_thread -ltorrent-rasterbar -dynamiclib -undefined suppress -flat_namespace bitmark_plugin/bitmark_plugin.cpp bitmark_plugin/bitmark_peer_data.cpp bitmark_plugin/tweetnacl.o -Wall -fpermissive -l json -lcurl -o session.dylib session.cpp


gcc -dynamiclib -undefined suppress -flat_namespace -o bitmark_plugin/tweetnacl.dylib bitmark_plugin/tweetnacl.c

g++ -lboost_system -lboost_thread -ltorrent-rasterbar -lGeoIP -dynamiclib -undefined suppress -flat_namespace bitmark_plugin/bitmark_plugin.cpp bitmark_plugin/bitmark_peer_data.cpp bitmark_plugin/tweetnacl.dylib -Wall -fpermissive -l json -lcurl -o session.dylib session.cpp

g++ -lboost_system -lboost_thread -ltorrent-rasterbar -dynamiclib -undefined suppress -flat_namespace -o peer_data.dylib peer_data.cpp


g++ -lboost_system -lboost_thread -ltorrent-rasterbar -lGeoIP -dynamiclib -undefined suppress -flat_namespace bitmark_plugin/bitmark_plugin.cpp bitmark_plugin/bitmark_peer_data.cpp bitmark_plugin/tweetnacl.dylib -Wall -fpermissive -ljson -lcurl -o session.dylib session.cpp

